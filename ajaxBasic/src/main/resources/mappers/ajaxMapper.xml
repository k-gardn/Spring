<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.care.ajaxBasic.AjaxMapper">
	<select id="exists">
		SELECT id FROM session_quiz WHERE id=#{id}
	</select>
	
	<insert id="jsonInsert" parameterType="com.care.ajaxBasic.AjaxVO">
		INSERT INTO json_table VALUES(#{title}, #{artist}, #{price})
	</insert>
	
	<delete id="jsonDelete">
		DELETE FROM json_table
	</delete>
	
	<select id="ex6" resultType="com.care.ajaxBasic.AjaxVO">
		SELECT * FROM json_table
	</select>
	
	<select id="quiz" resultType="com.care.ajaxBasic.AjaxVO">
		SELECT * FROM json_table
	</select>
	
	<select id="searchQuiz" resultType="com.care.ajaxBasic.AjaxVO">
		SELECT * FROM json_table WHERE LOWER(title) like LOWER('%'||#{search}||'%')
	</select>
</mapper>
